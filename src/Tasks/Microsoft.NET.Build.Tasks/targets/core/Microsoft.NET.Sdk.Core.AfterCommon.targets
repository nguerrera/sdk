<Project>
  <!-- Hook up support for targeting .NET Core for projects that don't use Microsoft.NET.Sdk, but do
       define a TargetFramework. -->
  <Import Project="$(MSBuildThisFileDirectory)..\Microsoft.NET.Sdk.targets"
          Condition="'$(UsingMicrosoftNETSdk)' != 'true' And '$(TargetFramework)' != ''"/>

  <Import Project="$(MSBuildThisFileDirectory)..\..\..\Microsoft.NET.Sdk\targets\Microsoft.NET.Sdk.targets"
          Condition="'$(UsingMicrosoftNETSdk)' != 'true' And '$(TargetFramework)' != ''"/>
</Project>
